    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <controls:DataGrid x:Name="dataList" ItemsSource="{Binding items, Mode=OneWay}" HeadersVisibility="Column" AutoGenerateColumns="True">
        
    </controls:DataGrid>
</UserControl>
